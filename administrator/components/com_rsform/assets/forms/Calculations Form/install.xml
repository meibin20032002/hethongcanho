	<task type="query"><![CDATA[UPDATE `#__rsform_forms` SET `FormLayout` = '<h2>{global:formtitle}</h2>\r\n{error}\r\n<!-- Do not remove this ID, it is used to identify the page so that the pagination script can work correctly -->\r\n<fieldset class=\"formHorizontal formContainer\" id=\"rsform_3_page_0\">\r\n	{Customer Info Title:body}\r\n	<div class=\"rsform-block rsform-block-email\">\r\n		<div class=\"formControlLabel\">{Email:caption}<strong class=\"formRequired\">(*)</strong></div>\r\n		<div class=\"formControls\">\r\n		<div class=\"formBody\">{Email:body}<span class=\"formValidation\">{Email:validation}</span></div>\r\n		<p class=\"formDescription\">{Email:description}</p>\r\n		</div>\r\n	</div>\r\n	{Phone Title:body}\r\n	<div class=\"rsform-block rsform-block-storage\">\r\n		<div class=\"formControlLabel\">{Storage:caption}</div>\r\n		<div class=\"formControls\">\r\n		<div class=\"formBody\">{Storage:body}<span class=\"formValidation\">{Storage:validation}</span></div>\r\n		<p class=\"formDescription\">{Storage:description}</p>\r\n		</div>\r\n	</div>\r\n	<div class=\"rsform-block rsform-block-color\">\r\n		<div class=\"formControlLabel\">{Color:caption}</div>\r\n		<div class=\"formControls\">\r\n		<div class=\"formBody\">{Color:body}<span class=\"formValidation\">{Color:validation}</span></div>\r\n		<p class=\"formDescription\">{Color:description}</p>\r\n		</div>\r\n	</div>\r\n	{Total Title:body}\r\n	<div class=\"rsform-block rsform-block-quantity\">\r\n		<div class=\"formControlLabel\">{Quantity:caption}</div>\r\n		<div class=\"formControls\">\r\n		<div class=\"formBody\">{Quantity:body}<span class=\"formValidation\">{Quantity:validation}</span></div>\r\n		<p class=\"formDescription\">{Quantity:description}</p>\r\n		</div>\r\n	</div>\r\n	<div class=\"rsform-block rsform-block-total\">\r\n		<div class=\"formControlLabel\">{Total:caption}</div>\r\n		<div class=\"formControls\">\r\n		<div class=\"formBody\">{Total:body}<span class=\"formValidation\">{Total:validation}</span></div>\r\n		<p class=\"formDescription\">{Total:description}</p>\r\n		</div>\r\n	</div>\r\n	<div class=\"rsform-block rsform-block-submit\">\r\n		<div class=\"formControlLabel\">{Submit:caption}</div>\r\n		<div class=\"formControls\">\r\n		<div class=\"formBody\">{Submit:body}<span class=\"formValidation\">{Submit:validation}</span></div>\r\n		<p class=\"formDescription\">{Submit:description}</p>\r\n		</div>\r\n	</div>\r\n</fieldset>\r\n', `FormLayoutName` = 'responsive', `FormLayoutAutogenerate` = '0', `ShowThankyou` = '1', `Thankyou` = '<p>Thank you for your order!</p>\r\n<table class=\"adminlist table\">\r\n<thead>\r\n<tr><th>Product</th><th>Quantity</th><th>Unit Price</th></tr>\r\n</thead>\r\n<tbody>\r\n<tr>\r\n	<td>Phone {Storage:value}</td>\r\n	<td>{Quantity:value}</td>\r\n	<td>${Storage:price}</td>\r\n</tr>\r\n<tr>\r\n	<td>Color: <em>{Color:value}</em></td>\r\n	<td></td>\r\n	<td>${Color:price}</td>\r\n</tr>\r\n<tr>\r\n	<td colspan=\"2\" style=\"text-align: right;\"><strong>Total:</strong></td>\r\n	<td>${Total:value}</td>\r\n</tr>\r\n</tbody>\r\n</table>', `ShowContinue` = '1' WHERE `FormId` = {FormId}]]></task>
